{
  "expo": {
    "name": "Phil E-Read",
    "slug": "Phil-E-Read",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/App-Logo/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/App-Logo/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/App-Logo/logo-transparent.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
